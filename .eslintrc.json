{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "env": {
    "jest": true
  },
  "rules": {
    "quotes": [
      2,
      "double",
      {
        "avoidEscape": true
      }
    ],
    "no-plusplus": "off"
  }
}
